<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE taglib PUBLIC 
  "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" 
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>DOM Tags</shortname>
	<tag>   
		<name>parse</name>
		<tagclass>tags.xml.dom.DOMParserTag</tagclass>
		<teiclass>tags.xml.dom.DOMParserTagInfo</teiclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>xmlFile</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>force</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>scope</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>   
		<name>iterate</name>
		<tagclass>tags.xml.dom.NodeIteratorTag</tagclass>
		<teiclass>tags.xml.dom.NodeIteratorTagInfo</teiclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>node</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>   
		<name>ifNodeNameEquals</name>
		<tagclass>tags.xml.dom.IfNodeNameEqualsTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>names</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>node</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>   
		<name>ifNodeIsElement</name>
		<tagclass>tags.xml.dom.IfNodeIsElementTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>node</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>   
		<name>elementValue</name>
		<tagclass>tags.xml.dom.GetElementValueTag</tagclass>
		<teiclass>tags.xml.dom.GetElementValueTagInfo</teiclass>
		<bodycontent>empty</bodycontent>
		<attribute>
			<name>element</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
