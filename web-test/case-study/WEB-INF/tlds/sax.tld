<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE taglib PUBLIC 
  "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" 
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>SAX Tags</shortname>
	<tag>   
		<name>iterateElements</name>
		<tagclass>tags.xml.sax.SAXParserTag</tagclass>
		<teiclass>tags.xml.sax.SAXParserTagInfo</teiclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>xmlFile</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>   
		<name>ifElementNameEquals</name>
		<tagclass>tags.xml.sax.IfElementNameEqualsTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>names</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>element</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>   
		<name>ifElementNameNotEquals</name>
		<tagclass>tags.xml.sax.IfElementNameNotEqualsTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>names</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>element</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
