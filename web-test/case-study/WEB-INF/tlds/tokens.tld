<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC 
  "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" 
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd"> 

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>Tokens for Detecting Sensitive Form Resubmission</shortname>
	<tag>   
		<name>token</name>
		<tagclass>tags.tokens.TokenTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>
	<tag>   
		<name>createTokens</name>
		<tagclass>tags.tokens.CreateTokensTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>
	<tag>   
		<name>checkTokens</name>
		<tagclass>tags.tokens.CheckTokensTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>
	<tag>   
		<name>removeSessionToken</name>
		<tagclass>tags.tokens.RemoveSessionTokenTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>
</taglib>
