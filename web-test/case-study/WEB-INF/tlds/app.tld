<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC 
  "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" 
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd"> 

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>Sun Microsystems Press Examples</shortname>

	<tag>   
		<name>hintAvailable</name>
		<tagclass>tags.app.HintAvailableTag</tagclass>
		<bodycontent>jsp</bodycontent>
	</tag>

	<tag>   
		<name>hintNotAvailable</name>
		<tagclass>tags.app.HintNotAvailableTag</tagclass>
		<bodycontent>jsp</bodycontent>
	</tag>   

	<tag>   
		<name>fruitPicker</name>
		<tagclass>tags.app.FruitPickerTag</tagclass>
		<teiclass>tags.app.FruitPickerTagInfo</teiclass>
		<bodycontent>jsp</bodycontent>

		<attribute>
			<name>caption</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>picture</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>   

	<tag>   
		<name>checkCart</name>
		<tagclass>tags.app.CheckCartTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>   

	<tag>   
		<name>iterateCart</name>
		<tagclass>tags.app.CartIteratorTag</tagclass>
		<teiclass>tags.app.CartIteratorTagInfo</teiclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>   
</taglib>
