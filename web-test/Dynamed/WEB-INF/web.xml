<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
    <servlet>
        <servlet-name>
            Dialogix
        </servlet-name>
        <servlet-class>
            org.dianexus.triceps.TricepsServlet
        </servlet-class>
        <init-param>
            <param-name>dialogix.dir</param-name>
            <param-value>/usr/local/dialogix/webapps/</param-value>
        </init-param>   
        <init-param>
            <param-name>scheduleSrcDir</param-name>
            <param-value>Dynamed/WEB-INF/schedules/</param-value>
        </init-param>
        <init-param>
            <param-name>workingFilesDir</param-name>
            <param-value>Dynamed/WEB-INF/working/</param-value>
        </init-param>
        <init-param>
            <param-name>completedFilesDir</param-name>
            <param-value>Dynamed/WEB-INF/completed/</param-value>
        </init-param>
        <init-param>
            <param-name>imageFilesDir</param-name>
            <param-value>/images/</param-value>
        </init-param>
        <init-param>
            <param-name>helpIcon</param-name>
            <param-value>help_true.gif</param-value>
        </init-param>
        <init-param>
            <param-name>logoIcon</param-name>
            <param-value>dynamednew.gif</param-value>
        </init-param>
        <init-param>
            <param-name>floppyDir</param-name>
            <param-value>/usr/local/dialogix/archive/</param-value>
        </init-param>
    </servlet>
</web-app>
