<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
    <servlet>
        <servlet-name>
            parseGAF
        </servlet-name>
        <servlet-class>
            org.dianexus.bprsgaf.parseGAF
        </servlet-class>
        <init-param>
            <param-name>dataDir</param-name>
            <param-value>@@DIALOGIX.DIR@@outcomes/WEB-INF/data/</param-value>
        </init-param>
    </servlet>
    
    <servlet>
        <servlet-name>
            parseBPRS
        </servlet-name>
        <servlet-class>
            org.dianexus.bprsgaf.parseBPRS
        </servlet-class>
        <init-param>
            <param-name>dataDir</param-name>
            <param-value>@@DIALOGIX.DIR@@outcomes/WEB-INF/data/</param-value>
        </init-param>
    </servlet>    
    <session-config>
        <session-timeout>
            600
        </session-timeout>
    </session-config>       
</web-app>



