CREATE TABLE instrument_session (
  instrument_session_id INTEGER(11) NOT NULL,
  start_time TIMESTAMP NOT NULL ,
  end_time TIMESTAMP NOT NULL ,
  istrument_id INTEGER(11) NOT NULL,
  user_id INTEGER(11) NOT NULL,
  first_group INTEGER(11) NOT NULL,
  last_group INTEGER(11) NOT NULL,
  last_action VARCHAR(20) NOT NULL,
  last_access VARCHAR(20) NOT NULL,
  statusMsg MEDIUMTEXT NOT NULL,
  PRIMARY KEY(instrument_session_id)
);

CREATE TABLE pagehitevents (
  pageHitEventsID INTEGER(11) NOT NULL,
  pageHitID INTEGER(11) NOT NULL,
  varName VARCHAR(40) NOT NULL,
  actionType VARCHAR(18) NOT NULL,
  eventType VARCHAR(18) NOT NULL,
  timestamp BIGINT(15) NOT NULL,
  duration INTEGER(11) NOT NULL,
  value1 VARCHAR(10) NOT NULL,
  value2 VARCHAR(50) NOT NULL,
  PRIMARY KEY(pageHitEventsID)
);

CREATE TABLE pagehits (
  pageHitID INTEGER(11) NOT NULL,
  instrument_session_id INTEGER(11) NOT NULL,
  timeStamp TIMESTAMP NOT NULL ,
  accessCount INTEGER(11) NOT NULL ,
  groupNum INTEGER(11) NOT NULL ,
  displayNum INTEGER(11) NOT NULL ,
  lastAction VARCHAR(15) NOT NULL,
  statusMsg VARCHAR(35) NOT NULL,
  totalDuration INTEGER UNSIGNED NULL,
  serverDuration INTEGER UNSIGNED NULL,
  loadDuration INTEGER UNSIGNED NULL,
  networkDuration INTEGER UNSIGNED NULL,
  pageVacillation INTEGER UNSIGNED NULL,
  PRIMARY KEY(pageHitID)
);

CREATE TABLE parkinsonsratingscale_v__n16_710bd9e9812c0cf0474607370f1af88a (
  session_id INTEGER UNSIGNED NOT NULL,
  instrument_session_id INTEGER(11) NOT NULL,
  InstrumentName VARCHAR(200) NOT NULL,
  InstanceName VARCHAR(200) NOT NULL,
  StartTime TIMESTAMP NOT NULL ,
  Intro TEXT NOT NULL,
  speech TEXT NOT NULL,
  facial TEXT NOT NULL,
  tremor TEXT NOT NULL,
  rigid_i TEXT NOT NULL,
  neck TEXT NOT NULL,
  Rarm TEXT NOT NULL,
  Larm TEXT NOT NULL,
  Rleg TEXT NOT NULL,
  Lleg TEXT NOT NULL,
  posture TEXT NOT NULL,
  kinesia TEXT NOT NULL,
  gait TEXT NOT NULL,
  score TEXT NOT NULL,
  sx_list TEXT NOT NULL,
  report TEXT NOT NULL,
  endTime TIMESTAMP NULL,
  firstGroup INTEGER UNSIGNED NULL,
  lastGroup INT NULL,
  lastAction INTEGER UNSIGNED NULL,
  lastAccess INTEGER UNSIGNED NULL,
  statusMsg TEXT NULL,
  PRIMARY KEY(session_id)
);

CREATE TABLE rawdata (
  RawDataID BIGINT(20) NOT NULL,
  instrument_session_id INTEGER(11) NOT NULL,
  InstrumentName VARCHAR(200) NOT NULL,
  InstanceName VARCHAR(200) NOT NULL,
  VarName VARCHAR(100) NOT NULL,
  VarNum INTEGER(11) NOT NULL,
  GroupNum SMALLINT(6) NOT NULL,
  DisplayNum SMALLINT(6) NOT NULL,
  LangNum SMALLINT(6) NOT NULL,
  WhenAsMS BIGINT(20) NOT NULL,
  TimeStamp TIMESTAMP NOT NULL ,
  AnswerType TINYINT(4) NOT NULL,
  Answer TEXT NOT NULL,
  QuestionAsAsked TEXT NOT NULL,
  itemVacillation INTEGER UNSIGNED NULL,
  responseLatency INTEGER UNSIGNED NULL,
  responseDuration INTEGER UNSIGNED NULL,
  nullFlavor INTEGER UNSIGNED NULL,
  Comment TEXT NOT NULL,
  PRIMARY KEY(RawDataID)
);

CREATE TABLE user session (
  user_session_id INTEGER  NOT NULL AUTO_INCREMENT,
  instrument_session_id INTEGER(11) NOT NULL,
  user_id INTEGER UNSIGNED NULL,
  timestamp TIMESTAMP NULL,
  comments MEDIUMTEXT NULL,
  s INTEGER UNSIGNED NULL,
  PRIMARY KEY(user_session_id)
);

